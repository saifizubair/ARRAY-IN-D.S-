package com.company;

import java.lang.*;
import java.util.*;
import java.io.*;


public class Main {

    public static void main(String[] args) {

        int arr[]={1,2,3,5,6};
        int expelements= arr.length+1;
        int totalsum=expelements*(expelements+1)/2;
        int sum=0;
        for (int i=0;i<arr.length;i++){
            sum=sum+arr[i];
        }
        System.out.println("missing number is "+(totalsum-sum));

    }
}
